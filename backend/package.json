  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "migrate:dev": "cross-env NODE_ENV=development knex migrate:latest --knexfile src/db/knexfile.cjs",
    "migrate:rollback:dev": "cross-env NODE_ENV=development knex migrate:rollback --knexfile src/db/knexfile.cjs",
    "seed:dev": "cross-env NODE_ENV=development knex seed:run --knexfile src/db/knexfile.cjs",
    "migrate:prod": "cross-env NODE_ENV=production knex migrate:latest --knexfile src/db/knexfile.cjs",
    "migrate:rollback:prod": "cross-env NODE_ENV=production knex migrate:rollback --knexfile src/db/knexfile.cjs",
    "seed:prod": "cross-env NODE_ENV=production knex seed:run --knexfile src/db/knexfile.cjs",
    "migrate:create": "cross-env NODE_ENV=development knex migrate:make --knexfile src/db/knexfile.cjs"
  },
